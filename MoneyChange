#include<iostream>
#include<string>
#include<iomanip>
using namespace std;
int main()
{
	float money,usd,eu;
	cout << "Input Thai Baths : ";
	cin >> money;
	usd = money*0.033;
	eu = money/34.64;
	cout << "****Exchange Rate****\n";
	cout << money << " Baths is " << setprecision(3) << usd << " dollars\n";
	cout << money << " Baths is " << setprecision(3) << eu << " Euro\n";
	return(0);
}
